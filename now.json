{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "/api/(.*)", "dest": "https://api.readhub.cn/$1" },
    {
      "src": "/(js|css)/.*",
      "headers": { "cache-control": "max-age=31536000, immutable" }
    },
    { "handle": "filesystem" },
    { "src": ".*", "dest": "/" }
  ],
  "regions": ["hnd1"],
  "alias": "readhub.now.sh"
}
